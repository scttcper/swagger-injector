{
  "name": "koa2-swagger-ui",
  "version": "0.0.0-placeholder",
  "description": "Swagger UI middleware for koa",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "ts-node test/example.ts",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --ext .ts,.js --fix .",
    "prepare": "npm run build",
    "build": "del-cli dist && tsc -p tsconfig.build.json",
    "postbuild": "cp lib/index.hbs dist && cp lib/favicon.png dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --no-cache"
  },
  "dependencies": {
    "@types/koa": "^2.11.4",
    "handlebars": "^4.7.6",
    "lodash.defaultsdeep": "^4.6.1",
    "read-pkg-up": "^7.0.1"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "1.1.0",
    "@jest/globals": "26.4.2",
    "@types/koa-router": "7.4.1",
    "@types/lodash.defaultsdeep": "4.6.6",
    "@types/node": "14.6.2",
    "@types/supertest": "2.0.10",
    "del-cli": "3.0.1",
    "jest": "26.4.2",
    "koa": "2.13.0",
    "koa-router": "9.4.0",
    "supertest": "4.0.2",
    "swagger-ui-dist": "3.32.5",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/scttcper/koa2-swagger-ui",
  "repository": "scttcper/koa2-swagger-ui",
  "keywords": [
    "swagger",
    "docs",
    "swagger-ui",
    "koa",
    "koa2",
    "middleware"
  ],
  "license": "MIT"
}
